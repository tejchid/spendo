generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Transaction {
  id            String   @id @default(uuid())
  date          DateTime
  merchantRaw   String
  merchantClean String
  amount        Float
  category      String
  source        String
  createdAt     DateTime @default(now())
}

model Insight {
  id        String   @id @default(uuid())
  type      String
  message   String
  month     String
  createdAt DateTime @default(now())
}
