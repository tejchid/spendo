generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Transaction {
  id        String   @id @default(cuid())
  amount    Float
  merchant  String?
  createdAt DateTime @default(now())
}

model Insight {
  id        String   @id @default(cuid())
  type      String
  message   String
  createdAt DateTime @default(now())
}
